@{
    ViewDate["Title"] - "Cadetes Page";
}

<div class = "text-center">
    <form asp-controller="Cadete" asp-action= "addCadete" method="post">
        <legend>"Registrar Cadete"</legend>
        <div>"Nombre" <input type="text" name="nom" required></div>
        <div>"Direccion"<input type="text" name= "dir" required></div>
        <div>"Telefono"<input type="number" name="tel" required></div>
        <div><input type="submit" value="Cargar" name="car"><input type="reset" value="Borrar" name="res"></div>
    </form>
</div>